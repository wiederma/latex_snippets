 doi = {10.17487/RFC5056},
 url = {https://tools.ietf.org/html/rfc5056},
 author = {Nicolas Williams},
 title = {{On the Use of Channel Bindings to Secure Channels}},
 pagetotal = 23,
 year = 2007,
 month = nov,
 urldate= {2017-08-23},
 abstract = {The concept of channel binding allows applications to establish that the two end-points of a secure channel at one network layer are the same as at a higher layer by binding authentication at the higher layer to the channel at the lower layer. This allows applications to delegate session protection to lower layers, which has various performance benefits.},
}

